<section class='compare'>
    {{#each copy.compare}}
    <p class='prose'>{{{value}}}</p>
    {{/each}}
    <div class='search-container'>
        <div class='directions'>
            <p class='title'>Am I a future Karen?</p>
            <p>Search for a name</p>
        </div>
        <label for='my-autocomplete'>Search for a name</label>
        <div class='autocomplete' id='autocomplete'></div>
        <p class='instruction'>If you don't see your name in the search, your name did not appear in our data.</p>
    </div>
    <div class='table-container'>
        <div class='table table__female'>
            <h3>Female Names</h3>
            <table data-gender='F'></table>
        </div>
        <div class='table table__male'>
            <h3>Male Names</h3>
            <table data-gender='M'></table>
        </div>
    </div>
</section>